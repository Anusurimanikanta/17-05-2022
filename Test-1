t=int(input())

while t:

  t-=1

  n=int(input())

  arr=[]

  arr.append(0)

  arr.extent(list(map(int,input().split())))

  def pivotindex(self,arr:List[int]) -> int:

    n=len(arr)

    pf=[0]*n

    pf[0]=arr[0]

    for i in range(1,n):

        pf[i]=pf[i-1]+arr[i]

    for i in range(n):

        if i==0:

            lsum=0

        else:

            lsum=pf[i-1]

            rsum=pf[n-1]-pf[i]

        if lsum==rsum:

    return-1
